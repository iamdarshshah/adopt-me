{
    "extends": [
        "eslint:recommended",
        "plugin: import/errors",
        "plugin: react/recommended",
        "olugin: jsx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "rules": {
        "react/props-types": 0,
        "no-console": 1
    },
    "plugins": ["react", "import", "jsx-a11y"],
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "settings": {
        "version": "detect"
    }
}